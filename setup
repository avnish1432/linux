# SSH	
git@github.com:avnish1432/linux.git

git config --global user.name "your_name"
git config --global user.email "your_email_address"
git config --list


#Git Repository setup code
git init
git checkout -b linux
git status
git add  
git commit -m "msg"
git remote add origin git@github.com:avnish1432/linux.git
git push -u origin linux
git branch
git branch -m linux
==========================

git init
git status
git add .
git commit -m "new"
git oust -u origin master

=========================

git init
git add <file_name>
git commit -m "first commit"
git remote add origin git@github.com:avnish1432/linux.git
git push -u origin master

