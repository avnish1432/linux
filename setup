# SSH	
git@github.com:avnish1432/linux.git

#or create a new repository on the command line
echo "# linux" >> README.md
git init
git add README.md
git commit -m "first commit"
git branch -M main
git remote add origin git@github.com:avnish1432/linux.git
git push -u origin main

#or push an existing repository from the command line
git remote add origin git@github.com:avnish1432/linux.git
git branch -M main
git push -u origin main

#Git Repository setup code
git init
git checkout -b linux
git status
git add  
git commit -m "msg"
git remote add origin git@github.com:avnish1432/linux.git
git push -u origin linux
git branch
git branch -m linux
==========================

git init
git status
git add .
git commit -m "new"
git oust -u origin master

=========================

git init
git add <file_name>
git commit -m "first commit"
git remote add origin git@github.com:avnish1432/linux.git
git push -u origin master


